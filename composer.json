{
    "name": "blue-strawberry\/query-mule",
    "type": "library",
    "description": "Database abstraction layer (DBAL) and fluent query builder for PHP. Developers can use this package on its own or in conjunction with a framework.",
    "keywords": [
        "query builder",
        "database abstraction layer",
        "dbal",
        "sql",
        "filter"
    ],
    "authors": [
        {
            "name": "Fraser Reid",
            "homepage": "https://freid001.github.io",
            "role": "Developer"
        }
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "QueryMule\\": "src\/"
        },
        "classmap": [
            "src/"
        ]
    },
    "require": {
        "php": ">=7.1",
        "ext-pdo": "*",
        "ext-mysqli": "*",
        "ext-json": "*",
        "psr/log": "^1.0",
        "psr/simple-cache": "^1.0",
        "monolog/monolog": "^1.23"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.4.3",
        "phpmetrics/phpmetrics": "^2.4",
        "phpstan/phpstan": "^0.9.2"
    }
}
