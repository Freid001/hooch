{
    "name": "blue-strawberry\/query-mule",
    "type": "library",
    "description": "Database abstraction layer (DBAL) and fluent query builder for PHP. Developers can use this package on its own or in conjunction with a framework.",
    "keywords": [
        "query builder",
        "database abstraction layer",
        "dbal",
        "sql"
    ],
    "authors": [
        {
            "name": "Freid001"
        }
    ],
    "license": "BSD-3-Clause",
    "autoload": {
        "psr-4": {
            "QueryMule\\": "src\/",
            "QueryMule\\Demo\\": "demo\/"
        },
        "classmap": [
            "src/"
        ]
    },
    "require": {
        "php": ">=7.0",
        "psr/log": "^1.0",
        "psr/simple-cache": "^1.0",
        "monolog/monolog": "^1.23"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8"
    },
    "minimum-stability": "stable",
    "version": "1.0.0"
}
